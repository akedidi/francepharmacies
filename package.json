{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:native": "vite build && npx @capacitor/cli sync",
    "lint": "eslint .",
    "preview": "vite preview",
    "cap:add:ios": "npx @capacitor/cli add ios",
    "cap:add:android": "npx @capacitor/cli add android",
    "cap:sync": "npx @capacitor/cli sync",
    "cap:open:ios": "npx @capacitor/cli open ios",
    "cap:open:android": "npx @capacitor/cli open android",
    "cap:run:ios": "npx @capacitor/cli run ios",
    "cap:run:android": "npx @capacitor/cli run android",
    "cap:run:ios:device": "npx @capacitor/cli run ios --target device",
    "cap:run:android:device": "npx @capacitor/cli run android --target device"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/ios": "^7.4.3",
    "@supabase/supabase-js": "^2.57.4",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
